<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/png" sizes="16x16" href="@Url.Content("~/eliteadmin/plugins/images/favicon.png")">
    <title>Elite Admin - is a responsive admin template</title>
    <!-- Bootstrap Core CSS -->
    <link href="@Url.Content("~/eliteadmin/eliteadmin-horizontal-navbar/bootstrap/dist/css/bootstrap.min.css")" rel="stylesheet">
    <!-- animation CSS -->
    <link href="@Url.Content("~/eliteadmin/eliteadmin-horizontal-navbar/css/animate.css")" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="@Url.Content("~/eliteadmin/eliteadmin-horizontal-navbar/css/style.css")" rel="stylesheet">
    <!-- color CSS -->
    <link href="@Url.Content("~/eliteadmin/eliteadmin-horizontal-navbar/css/colors/megna.css")" id="theme" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="http://www.w3schools.com/lib/w3data.js"></script>
</head>
<body>
    <!-- Preloader -->
    <div class="preloader">
        <div class="cssload-speeding-wheel"></div>
    </div>
    <section id="wrapper" class="login-register" style="max-width: 100%">
        <div class="login-box">
            @RenderBody()
        </div>
    </section>
    <!-- jQuery -->
    <script src="@Url.Content("~/eliteadmin/plugins/bower_components/jquery/dist/jquery.min.js")"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="@Url.Content("bootstrap/dist/js/bootstrap.min.js")"></script>
    <!-- Menu Plugin JavaScript -->
    <script src="@Url.Content("~/eliteadmin/plugins/bower_components/sidebar-nav/dist/sidebar-nav.min.js")"></script>
    <!--Wave Effects -->
    <script src="@Url.Content("~/eliteadmin/eliteadmin-horizontal-navbar/js/waves.js")"></script>
    <!-- Custom Theme JavaScript -->
    <script src="@Url.Content("~/eliteadmin/eliteadmin-horizontal-navbar/js/custom.js")"></script>
    <!--Style Switcher -->
    <script src="@Url.Content("~/eliteadmin/plugins/bower_components/styleswitcher/jQuery.style.switcher.js")"></script>
</body>
</html>